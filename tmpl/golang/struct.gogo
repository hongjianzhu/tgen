{{define "struct" -}}
{{- $data := .}}
{{- range $oneStruct := .Structs}}
type {{$data.UpperHead $oneStruct.Name}} struct {
    {{- range $field := $oneStruct.Fields}}
    {{$data.UpperHead $field.Name}} {{.GenTypeString $field.Name $field.Type nil $field.Optional}} `thrift:"{{$data.FieldTagThrift $field}}" json:"{{$data.FieldTagJson $field}}"`
    {{- end}}
}
{{- end}}
{{end}}
